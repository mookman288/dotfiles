[alias]
    a   = add
    aa  = !git add -u && git add . && git status
    bd  = branch -D
    bm  = branch -m
    c   = commit -m
    ca  = commit --amend -m
    cd  = cd "`git rev-parse --show-toplevel`"
    co  = checkout
    cob = checkout -b
    cp  = cherry-pick
    d   = diff | subl
    fp  = fetch --prune
    fl  = log -u
    ir  = !git rebase -i
    l   = log --pretty=format:'%C(green)%h%C(reset) %s %Cblue[%an]%Creset' --graph
    lf  = log --pretty=format:'%C(green)%h%C(reset)%C(yellow)%d%C(reset) %s %C(blue)[%cn]%C(reset)' --decorate --numstat
    lg  = log --pretty=format:'%C(green)%h%C(reset)%C(yellow)%d%C(reset) %s %C(red)(%cr)%C(reset) %C(blue)[%an]%C(reset)' --abbrev-commit --graph
    mq  = merge --squash
    pr  = pull --rebase
    rh  = reset --hard
    s   = status
    sa  = stash apply
    sb  = stash branch
    sc  = stash clear
    sl  = stash list
    sp  = stash pop
    ss  = stash save
    um  = ls-files -u
[color]
    ui = true
[core]
    excludesfile = /Users/rxgx/.gitignore
    editor       = atom --wait
[merge]
    conflictstyle = diff3
[user]
    name  = Ryan Gasparini
    email = ryan@rxgx.com
